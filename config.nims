when defined nimscript:
  hint "Processing", off
  hint "GlobalVar", on
  hint "Performance", on
  switch "verbosity", "0"
  switch "styleCheck", "off"
  # switch "excessiveStackTrace", "off"

  switch "define", "ssl"
  switch "define", "withGitHubUploader"
  switch "define", "usesodium"
  # switch "define", "release"
  switch "threads", "on"
